{
    "version": "0.2.0",
    "configurations": [

      {
        "type": "chrome",
        "request": "launch",
        "name": "Launch Chrome against localhost",
        "url": "http://localhost:4000",
        "webRoot": "${workspaceFolder}"
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Run yarn start:dev",
        "runtimeExecutable": "yarn",
        "args": ["start:dev"],
        "cwd": "${workspaceFolder}",
        "protocol": "inspector",
        "console": "integratedTerminal"
      }
    ]
  }
  